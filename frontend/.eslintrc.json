{
	"extends": [
    "eslint:recommended",
    "plugin:react/recommended"
	],
	"plugins": [
    "react",
    "import"
  ],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module",
    "ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
	"rules": {
		"require-jsdoc": "off",
		"react/prop-types": "off",
		"import/no-unresolved": 0
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
